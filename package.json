{
  "name": "spotify-alchemist",
  "version": "1.0.0",
  "description": "An intelligent playlist generation tool that creates custom Spotify playlists using natural language prompts powered by OpenAI's GPT-4. Now available as a native macOS desktop app with fluid UI.",
  "main": "electron/main.js",
  "homepage": "https://github.com/Lachlan2252/SpotifyAlchemist",
  "author": {
    "name": "Lachlan2252",
    "email": "lachlan@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lachlan2252/SpotifyAlchemist.git"
  },
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build",
    "preview": "vite preview",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "electron": "electron electron/main.js",
    "electron:dev": "NODE_ENV=development electron electron/main.js",
    "electron:pack": "npm run build && electron-builder --mac --publish=never",
    "electron:dist": "npm run build && electron-builder --mac --publish=onTagOrDraft",
    "release:test": "bash scripts/test-release.sh"
  },
  "keywords": ["spotify", "playlist", "ai", "music", "electron", "macos", "desktop"],
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@jridgewell/trace-mapping": "^0.3.29",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.83.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^0.2.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.34.0",
    "embla-carousel-react": "^8.6.0",
    "esbuild": "^0.25.6",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "framer-motion": "^12.23.6",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.525.0",
    "memoizee": "^0.4.17",
    "memorystore": "^1.6.7",
    "next-themes": "^0.4.6",
    "openai": "^5.10.1",
    "openid-client": "^6.6.2",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-icons": "^5.5.0",
    "react-resizable-panels": "^3.0.3",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "vaul": "^1.1.2",
    "vite": "^6.3.5",
    "wouter": "^3.7.1",
    "ws": "^8.18.3",
    "zod": "^3.25.1",
    "zod-validation-error": "^3.5.3"
  },
  "devDependencies": {
    "@types/express-session": "^1.18.2",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.8",
    "electron": "^37.2.3",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.lachlan2252.spotify-alchemist",
    "productName": "Spotify Alchemist",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.music",
      "icon": "electron/assets/icon.png",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        }
      ],
      "darkModeSupport": true,
      "gatekeeperAssess": false,
      "hardenedRuntime": true,
      "entitlements": "electron/entitlements.mac.plist",
      "entitlementsInherit": "electron/entitlements.mac.plist"
    },
    "dmg": {
      "title": "Spotify Alchemist",
      "backgroundColor": "#1a1a2e",
      "window": {
        "width": 600,
        "height": 400
      }
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
